#include<stdio.h>
void swap(int* px,int* py)
{
    int z = *px;
    *px = *py;
    *py = z;

}
//void swap(int x,int y)错误版本
//{
//     int z=0;
//     z = x;
 //    x = y;
 //    y = z;

//}
//当实参传递给形参的时候，形参是实参的一份临时拷贝
//形参的修改不会 影响实参
int main()
{
    int a = 0;
    int b = 0;

    scanf("%d %d",&a,&b);
    //a和b是实参x和y是形参
    printf("交换前: a =%d b = %d\n ",a ,b);
   // swap(a,b);
    swap(&a,&b);
    printf("交换后:a =%d b =%d\n",a,b);

    return 0;
}
